{
  "name": "boogiehook",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler deploy",
    "dev": "remix dev --manual -c \"npm start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler dev ./build/index.js",
    "typecheck": "tsc",
    "migrate-dev": "drizzle-kit generate:sqlite --schema=app/db/schema.server.ts --out=./migrations && wrangler d1 migrations apply DB --local",
    "migrate": "yarn migrate-dev && wrangler d1 migrations apply DB"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@discordjs/rest": "^2.2.0",
    "@emoji-mart/data": "^1.1.2",
    "@remix-run/cloudflare": "^2.4.0",
    "@remix-run/css-bundle": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "@twemoji/api": "^15.0.3",
    "@types/showdown": "^2.0.6",
    "discord-api-types": "^0.37.66",
    "discord-snowflake": "^2.0.0",
    "drizzle-orm": "^0.29.1",
    "emoji-mart": "^5.5.2",
    "isbot": "^4.3.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-emoji-search": "^2.2.26",
    "react-modal": "^3.16.1",
    "react-select": "^5.8.0",
    "react-showdown": "^2.3.1",
    "remix-auth-discord": "^1.3.2",
    "zod": "^3.22.4",
    "zodix": "^0.4.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.4.0",
    "@types/react": "^18.2.20",
    "@types/react-color": "^3.0.10",
    "@types/react-dom": "^18.2.7",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.1.6",
    "wrangler": "^3.21.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
